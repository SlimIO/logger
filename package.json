{
  "name": "@slimio/logger",
  "version": "0.1.2",
  "description": "SlimIO Sonic Logger",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "prepublishOnly": "pkg-ok",
    "test": "cross-env psp && node test/test.js",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/logger.git"
  },
  "keywords": [
    "SlimIO",
    "logger",
    "sonic",
    "low-overhead"
  ],
  "files": [
    "index.js",
    "index.d.ts",
    "src"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/logger/issues"
  },
  "homepage": "https://github.com/SlimIO/logger#readme",
  "dependencies": {
    "flatstr": "^1.0.12",
    "kleur": "^3.0.3",
    "sonic-boom": "^0.7.6",
    "snyk": "^1.320.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^4.1.0",
    "@slimio/is": "^1.5.1",
    "@slimio/psp": "^0.11.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.0.0",
    "husky": "^4.2.5",
    "japa": "^3.0.1",
    "jsdoc": "^3.6.4",
    "nyc": "^15.0.1",
    "pkg-ok": "^2.3.1",
    "premove": "^1.0.0"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env eslint index.js && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  },
  "snyk": true
}
